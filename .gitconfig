[user]
	name = louis
	email = louis@timetreeapp.com
	signingkey = 0D76F77A7076F9C40B3403A90E58B0BF295B5732
[core]
    pager = lv -c
    editor = vim
	excludesfile = /Users/louis/.gitignore_global
[color]
    ui = auto
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
    tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
    st = status
    sh = show
    ci = commit -a
    co = checkout
    br = branch
    ba = branch -a
    dm = diff master
    dw = diff --color-words
    dc = diff --cached
    d1 = diff HEAD~
    d2 = diff HEAD~~
    d3 = diff HEAD~~~
    d4 = diff HEAD~~~~
    d5 = diff HEAD~~~~~
    gr = grep
    gn = grep -n
[include]
    path = ~/.gitconfig.local
[commit]
	template = /Users/louis/.stCommitMsg
	gpgsign = true
[gpg]
	program = gpg
